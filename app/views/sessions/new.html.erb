<h1><%= t('session.login') %></h1>
<div class="row">
	<div class= "col-md-6 col-md-offset-3">

<%= form_tag '/login' do %>

  <%= t('session.email') %> <%= text_field_tag :email %>
  <%= t('session.password') %> <%= password_field_tag :password %>
  <%= submit_tag t("session.submit"), class: "btn btn-primary" %>

<% end %>
</div>
</div>
